{"ast":null,"code":"import { ref } from \"vue\";\nexport default {\n  name: \"CreatePost\",\n  setup() {\n    const post = ref({\n      firstName: \"\",\n      lastName: \"\",\n      email: \"\",\n      text: \"\",\n      likes: 0,\n      tags: \"\",\n      publishDate: \"\",\n      post: \"\"\n    });\n    const createPost = async () => {\n      try {\n        // First, create the user\n        const userResponse = await fetch(\"https://dummyapi.io/data/v1/user/create\", {\n          method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n            \"app-id\": \"YOUR_APP_ID\" // Replace with your actual API ID\n          },\n\n          body: JSON.stringify({\n            firstName: post.value.firstName,\n            lastName: post.value.lastName,\n            email: post.value.email\n          })\n        });\n        if (!userResponse.ok) {\n          throw new Error(\"Failed to create user\");\n        }\n        const user = await userResponse.json();\n\n        // Update the post object with the created user's ID\n        post.value.owner = user.id;\n\n        // Create the post\n        const postResponse = await fetch(\"https://dummyapi.io/data/v1/post/create\", {\n          method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n            \"app-id\": \"YOUR_APP_ID\" // Replace with your actual API ID\n          },\n\n          body: JSON.stringify(post.value)\n        });\n        if (!postResponse.ok) {\n          throw new Error(\"Failed to create post\");\n        }\n\n        // Handle success or redirect to home page\n      } catch (error) {\n        console.error(error);\n      }\n    };\n    return {\n      post,\n      createPost\n    };\n  }\n};","map":{"version":3,"names":["ref","name","setup","post","firstName","lastName","email","text","likes","tags","publishDate","createPost","userResponse","fetch","method","headers","body","JSON","stringify","value","ok","Error","user","json","owner","id","postResponse","error","console"],"sources":["/Users/Abbe/Self techover/vue/frontend-coding-test/src/components/CreatePost.vue"],"sourcesContent":["<template>\n  <div class=\"create-post\">\n    <h2>Create Post</h2>\n    <form @submit.prevent=\"createPost\" class=\"post-form\">\n      <div class=\"form-group\">\n        <label for=\"firstName\">Owner First Name</label>\n        <input\n          type=\"text\"\n          id=\"firstName\"\n          v-model=\"post.owner.firstName\"\n          required\n        />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"lastName\">Owner Last Name</label>\n        <input\n          type=\"text\"\n          id=\"lastName\"\n          v-model=\"post.owner.astName\"\n          required\n        />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"email\">Owner Email</label>\n        <input type=\"email\" id=\"email\" v-model=\"post.email\" required />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"text\">Text</label>\n        <textarea id=\"text\" v-model=\"post.text\" required></textarea>\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"likes\">Likes</label>\n        <input type=\"number\" id=\"likes\" v-model=\"post.likes\" required />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"tags\">Tags</label>\n        <input type=\"text\" id=\"tags\" v-model=\"post.tags\" required />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"publishDate\">Publish Date</label>\n        <input\n          type=\"date\"\n          id=\"publishDate\"\n          v-model=\"post.publishDate\"\n          required\n        />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"post\">Post</label>\n        <textarea id=\"post\" v-model=\"post.post\" required></textarea>\n      </div>\n\n      <button type=\"submit\" class=\"submit-button\">Create</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport { ref } from \"vue\";\n\nexport default {\n  name: \"CreatePost\",\n  setup() {\n    const post = ref({\n      firstName: \"\",\n      lastName: \"\",\n      email: \"\",\n      text: \"\",\n      likes: 0,\n      tags: \"\",\n      publishDate: \"\",\n      post: \"\",\n    });\n\n    const createPost = async () => {\n      try {\n        // First, create the user\n        const userResponse = await fetch(\n          \"https://dummyapi.io/data/v1/user/create\",\n          {\n            method: \"POST\",\n            headers: {\n              \"Content-Type\": \"application/json\",\n              \"app-id\": \"YOUR_APP_ID\", // Replace with your actual API ID\n            },\n            body: JSON.stringify({\n              firstName: post.value.firstName,\n              lastName: post.value.lastName,\n              email: post.value.email,\n            }),\n          }\n        );\n\n        if (!userResponse.ok) {\n          throw new Error(\"Failed to create user\");\n        }\n\n        const user = await userResponse.json();\n\n        // Update the post object with the created user's ID\n        post.value.owner = user.id;\n\n        // Create the post\n        const postResponse = await fetch(\n          \"https://dummyapi.io/data/v1/post/create\",\n          {\n            method: \"POST\",\n            headers: {\n              \"Content-Type\": \"application/json\",\n              \"app-id\": \"YOUR_APP_ID\", // Replace with your actual API ID\n            },\n            body: JSON.stringify(post.value),\n          }\n        );\n\n        if (!postResponse.ok) {\n          throw new Error(\"Failed to create post\");\n        }\n\n        // Handle success or redirect to home page\n      } catch (error) {\n        console.error(error);\n      }\n    };\n\n    return {\n      post,\n      createPost,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.create-post {\n  max-width: 400px;\n  margin: 0 auto;\n}\n\nh2 {\n  font-size: 24px;\n  margin-bottom: 20px;\n}\n\n.post-form {\n  display: flex;\n  flex-direction: column;\n}\n\n.form-group {\n  margin-bottom: 20px;\n}\n\nlabel {\n  font-weight: bold;\n  margin-bottom: 5px;\n}\n\ninput[type=\"text\"],\ninput[type=\"date\"],\ninput[type=\"email\"] {\n  width: 100%;\n  padding: 10px;\n  font-size: 16px;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n}\n\ntextarea {\n  width: 100%;\n  padding: 10px;\n  font-size: 16px;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n  resize: vertical;\n}\n\n.submit-button {\n  padding: 10px 20px;\n  background-color: #4caf50;\n  color: white;\n  border: none;\n  border-radius: 4px;\n  font-size: 16px;\n  cursor: pointer;\n}\n\n.submit-button:hover {\n  background-color: #45a049;\n}\n</style>\n"],"mappings":"AAiEA,SAASA,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAKA,CAAA,EAAG;IACN,MAAMC,IAAG,GAAIH,GAAG,CAAC;MACfI,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,CAAC;MACRC,IAAI,EAAE,EAAE;MACRC,WAAW,EAAE,EAAE;MACfP,IAAI,EAAE;IACR,CAAC,CAAC;IAEF,MAAMQ,UAAS,GAAI,MAAAA,CAAA,KAAY;MAC7B,IAAI;QACF;QACA,MAAMC,YAAW,GAAI,MAAMC,KAAK,CAC9B,yCAAyC,EACzC;UACEC,MAAM,EAAE,MAAM;UACdC,OAAO,EAAE;YACP,cAAc,EAAE,kBAAkB;YAClC,QAAQ,EAAE,aAAa,CAAE;UAC3B,CAAC;;UACDC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAAC;YACnBd,SAAS,EAAED,IAAI,CAACgB,KAAK,CAACf,SAAS;YAC/BC,QAAQ,EAAEF,IAAI,CAACgB,KAAK,CAACd,QAAQ;YAC7BC,KAAK,EAAEH,IAAI,CAACgB,KAAK,CAACb;UACpB,CAAC;QACH,CACF,CAAC;QAED,IAAI,CAACM,YAAY,CAACQ,EAAE,EAAE;UACpB,MAAM,IAAIC,KAAK,CAAC,uBAAuB,CAAC;QAC1C;QAEA,MAAMC,IAAG,GAAI,MAAMV,YAAY,CAACW,IAAI,CAAC,CAAC;;QAEtC;QACApB,IAAI,CAACgB,KAAK,CAACK,KAAI,GAAIF,IAAI,CAACG,EAAE;;QAE1B;QACA,MAAMC,YAAW,GAAI,MAAMb,KAAK,CAC9B,yCAAyC,EACzC;UACEC,MAAM,EAAE,MAAM;UACdC,OAAO,EAAE;YACP,cAAc,EAAE,kBAAkB;YAClC,QAAQ,EAAE,aAAa,CAAE;UAC3B,CAAC;;UACDC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAACf,IAAI,CAACgB,KAAK;QACjC,CACF,CAAC;QAED,IAAI,CAACO,YAAY,CAACN,EAAE,EAAE;UACpB,MAAM,IAAIC,KAAK,CAAC,uBAAuB,CAAC;QAC1C;;QAEA;MACF,E