{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport { fetchPostById } from \"./api.js\";\nexport default {\n  name: \"PostDetails\",\n  props: {\n    postId: {\n      type: String,\n      required: true\n    }\n  },\n  setup(props) {\n    const post = ref(null);\n    const fetchPost = async () => {\n      try {\n        post.value = await fetchPostById(props.postId);\n      } catch (error) {\n        console.error(\"Failed to fetch post:\", error);\n      }\n    };\n    onMounted(() => {\n      fetchPost();\n    });\n    const formatDate = dateString => {\n      const options = {\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\"\n      };\n      return new Date(dateString).toLocaleDateString(undefined, options);\n    };\n    return {\n      post,\n      formatDate\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","fetchPostById","name","props","postId","type","String","required","setup","post","fetchPost","value","error","console","formatDate","dateString","options","year","month","day","Date","toLocaleDateString","undefined"],"sources":["/Users/Abbe/Self techover/vue/frontend-coding-test/src/components/PostDetails.vue"],"sourcesContent":["<template>\n  <div class=\"post-details\">\n    <h2>{{ post.owner.title }}</h2>\n    <div class=\"owner-info\">\n      <img :src=\"post.picture\" :alt=\"post.firstName\" class=\"owner-avatar\" />\n      <p class=\"owner-name\">\n        By {{ post.owner.firstName }} {{ post.owner.lastName }}\n      </p>\n    </div>\n    <p class=\"post-text\">{{ post.text }}</p>\n    <p class=\"post-tags\">Tags: {{ post.tags.join(\", \") }}</p>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted } from \"vue\";\nimport { fetchPostById } from \"./api.js\";\nexport default {\n  name: \"PostDetails\",\n  props: {\n    postId: {\n      type: String,\n      required: true,\n    },\n  },\n  setup(props) {\n    const post = ref(null);\n\n    const fetchPost = async () => {\n      try {\n        post.value = await fetchPostById(props.postId);\n      } catch (error) {\n        console.error(\"Failed to fetch post:\", error);\n      }\n    };\n\n    onMounted(() => {\n      fetchPost();\n    });\n\n    const formatDate = (dateString) => {\n      const options = { year: \"numeric\", month: \"long\", day: \"numeric\" };\n      return new Date(dateString).toLocaleDateString(undefined, options);\n    };\n\n    return {\n      post,\n      formatDate,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.post-details {\n  max-width: 800px;\n  margin: 0 auto;\n  padding: 20px;\n}\n\n.publish-date {\n  color: #777;\n  font-size: 14px;\n  margin-bottom: 8px;\n}\n\n.owner-info {\n  display: flex;\n  align-items: center;\n  margin-bottom: 10px;\n}\n\n.owner-avatar {\n  width: 40px;\n  height: 40px;\n  border-radius: 50%;\n  margin-right: 10px;\n}\n\n.owner-name {\n  font-size: 16px;\n  font-weight: bold;\n  color: #333;\n}\n\n.post-text {\n  font-size: 16px;\n  color: #555;\n  margin-bottom: 10px;\n}\n\n.post-tags {\n  font-size: 14px;\n  color: #777;\n}\n</style>\n"],"mappings":"AAeA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,aAAY,QAAS,UAAU;AACxC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,KAAKA,CAACL,KAAK,EAAE;IACX,MAAMM,IAAG,GAAIV,GAAG,CAAC,IAAI,CAAC;IAEtB,MAAMW,SAAQ,GAAI,MAAAA,CAAA,KAAY;MAC5B,IAAI;QACFD,IAAI,CAACE,KAAI,GAAI,MAAMV,aAAa,CAACE,KAAK,CAACC,MAAM,CAAC;MAChD,EAAE,OAAOQ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF,CAAC;IAEDZ,SAAS,CAAC,MAAM;MACdU,SAAS,CAAC,CAAC;IACb,CAAC,CAAC;IAEF,MAAMI,UAAS,GAAKC,UAAU,IAAK;MACjC,MAAMC,OAAM,GAAI;QAAEC,IAAI,EAAE,SAAS;QAAEC,KAAK,EAAE,MAAM;QAAEC,GAAG,EAAE;MAAU,CAAC;MAClE,OAAO,IAAIC,IAAI,CAACL,UAAU,CAAC,CAACM,kBAAkB,CAACC,SAAS,EAAEN,OAAO,CAAC;IACpE,CAAC;IAED,OAAO;MACLP,IAAI;MACJK;IACF,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}